{
    "name": "wtsergo/amp-opensearch",
    "description": "opensearch client connection handler backed with amphp/http-client",
    "type": "library",
    "require": {
        "php": "^8.2",
        "amphp/http-client": "^5.1",
        "opensearch-project/opensearch-php": "^2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Boyko",
            "email": "wtsergo@gmail.com"
        }
    ],
    "autoload": {
        "files": [
            "src/functions.php"
